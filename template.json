[{
        "type": 1,
        "title": "WireGuard",
        "name": "wireguard",
        "description": "WireGuard is an extremely simple yet fast and modern VPN that utilizes state-of-the-art cryptography.",
        "logo": "https://cdn.freelogovectors.net/wp-content/uploads/2021/10/wireguard-logo-freelogovectors.net_-400x400.png",
        "image": "linuxserver/wireguard:latest",
        "categories": [
            "Other"
        ],
        "platform": "linux",
        "restart_policy": "unless-stopped",
        "ports": [{
                "VPN Tunnel": "51820:51820/udp"
            }
        ],
        "volumes": [{
                "container": "/config",
                "bind": "!config/WireGuard"
            }, {
                "container": "/lib/modules",
                "bind": "/lib/modules"
            }
        ],
        "sysctls": [{
                "net.ipv4.conf.all.src_valid_mark": "1"
            }
        ],
        "cap_add": [
                "NET_ADMIN",
                "SYS_MODULE"
        ],
        "env": [{
                "name": "PUID",
                "label": "PUID",
                "default": "!PUID"
            }, {
                "name": "PGID",
                "label": "PGID",
                "default": "!PGID"
            }, {
                "name": "SERVERPORT",
                "label": "SERVERPORT",
                "default": "51820"
            }, {
                "name": "SERVERURL",
                "label": "SERVERURL",
                "default": "auto"
            }, {
                "name": "PEERS",
                "label": "PEERS",
                "default": "1"
            }, {
                "name": "PEERDNS",
                "label": "PEERDNS",
                "default": "auto"
            }, {
                "name": "INTERNAL_SUBNET",
                "label": "INTERNAL_SUBNET",
                "default": "10.13.13.0"
            }, {
                "name": "ALLOWEDIPS",
                "label": "ALLOWEDIPS",
                "default": "0.0.0.0/0"
            }, {
                "name": "LOG_CONFS",
                "label": "LOG_CONFS",
                "default": "true"
            },
        ]
    }
]
